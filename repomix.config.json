{
  "output": {
    "filePath": "repomix-output.txt",
    "style": "xml"
  },
  "include": ["**/*"],
  "ignore": {
    "customPatterns": [
      "**/node_modules/**",
      "**/.git/**",
      "**/dist/**",
      "**/build/**",
      "**/.next/**",
      "**/package-lock.json",
      "**/yarn.lock",
      "**/pnpm-lock.yaml",
      "**/bun.lockb",
      "**/repomix-output.txt",
      "**/*.log",
      "**/.env*",
      "**/.vibe/**"
    ]
  }
}
